---
layout: base.njk
---

<h2>Pages</h2>
<ul>
  {% for item in search.pages("type=page", "id") %}
  <li><a href="{{ item.data.url }}">{{ item.data.id }} - {{ item.data.title }}</a></li>
  {% endfor %}
</ul>

